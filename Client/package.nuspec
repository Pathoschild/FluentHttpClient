    <projectUrl>https://github.com/Pathoschild/FluentHttpClient#readme</projectUrl>
    <iconUrl>https://upload.wikimedia.org/wikipedia/commons/thumb/d/d0/Noun_project_3067.svg/128px-Noun_project_3067.svg.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>wcf web webapi httpclient</tags>
    <dependencies>
